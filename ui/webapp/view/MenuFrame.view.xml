<mvc:View controllerName="cerpass.ui.controller.MenuFrame" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" 
	xmlns:core="sap.ui.core" 
	xmlns:tnt="sap.tnt" 
	displayBlock="true" 
	xmlns="sap.m">
	<tnt:ToolPage id="toolPage">
		<tnt:header></tnt:header>
		<tnt:sideContent>
			
			<tnt:SideNavigation expanded="true"
								itemSelect="onItemSelect">
				<tnt:NavigationList items="{path: '/MainMenu'}">
					<tnt:NavigationListItem text="{title}"
											icon="{icon}"
											enabled="{enabled}"
											expanded="{expanded}"
											items="{submenu}"
											key="{key}">
						<tnt:NavigationListItem text="{title}"
												key="{key}"
												enabled="{enabled}">
						</tnt:NavigationListItem>
					</tnt:NavigationListItem>
				</tnt:NavigationList>
				<tnt:fixedItem>
					<tnt:NavigationList items="{path: '/fixedNavigation'}">
						<tnt:NavigationListItem text="{title}"
												icon="{icon}">
						</tnt:NavigationListItem>
					</tnt:NavigationList>
				</tnt:fixedItem>
			</tnt:SideNavigation>
		</tnt:sideContent>	
		<tnt:mainContents>
			<NavContainer id="pageContainer">
				<pages>

				</pages>
			</NavContainer>
		</tnt:mainContents>
	</tnt:ToolPage>		
</mvc:View>