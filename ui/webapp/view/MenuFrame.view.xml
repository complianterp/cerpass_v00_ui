<mvc:View controllerName="cerpass.ui.controller.MenuFrame" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}" binding="{/MainMenu}">
				<content>
					<List items="{/MainMenu}">
						<StandardListItem type="Active" press="handleListItemPress" title="{title}"/>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>